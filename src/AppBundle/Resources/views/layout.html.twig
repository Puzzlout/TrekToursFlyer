{% extends '::base.html.twig' %}
{% block title %}Trek Tours Flyer - {% endblock %}
{% block stylesheets %}
    {{ parent() }}
    {% stylesheets
        'assets/vendor/bootstrap/dist/css/bootstrap.min.css'
        'bundles/app/css/*'
        filter='cssrewrite'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    {% javascripts
        'assets/vendor/jquery/dist/jquery.min.js'
        'assets/vendor/bootstrap/dist/js/bootstrap.min.js'
        'bundles/app/js/*'
    %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
{% block body %}
    {% block header %}
        <header>
            {{ render(controller('AppBundle:CommonModule:headerMenu')) }}
        </header>
    {% endblock %}
    {% block content %}{% endblock %}
    {% block footer %}
        {{ render(controller('AppBundle:CommonModule:footer')) }}
    {% endblock %}
{% endblock %}